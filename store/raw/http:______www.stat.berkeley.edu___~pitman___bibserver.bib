	BIBNUMBER = {131},
	BOOKTITLE = {{Representation Theory, Dynamical Systems, Combinatorial and Algorithmic Methods. Part 12}},
   SERIES = {Zap. Nauchn. Sem. S.-Peterburg. Otdel. Mat. Inst. Steklov.
              (POMI)},
    VOLUME = {325},
